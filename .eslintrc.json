{ 
  "env": {"es6":true}, 
  "extends": ["airbnb", "prettier"],  
  "plugins": ["prettier"], 
  "rules": {  
    "prettier/prettier": ["error", {
      "endOfLine": "auto"
    }],
    "import/prefer-default-export": "off",
    "no-param-reassign":"off",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": 0 
  }
}  